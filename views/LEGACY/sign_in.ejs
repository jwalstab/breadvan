<body>
  <!-- **********************************************************************************************************************************************************
      MAIN CONTENT
      *********************************************************************************************************************************************************** -->
  <div id="login-page">
    <div class="container">
      <form class="form-login" action="/logincheck" method='post'>
        <h2 class="form-login-heading">Quantum Data Technologies Secure Login</h2>
        <div class="login-wrap">
          <input type="text" class="form-control" placeholder="User ID" name="user" autofocus>
          <br>
          <input type="password" class="form-control" placeholder="Password" name="pass">
          <br>
          <button class="btn btn-theme btn-block" href="control" type="submit"><i class="fa fa-lock"></i> SIGN IN</button>
          <hr>
          <h6><%= status %></h6>
        </div>
      </form>
    </div>
  </div>
  <!-- js placed at the end of the document so the pages load faster -->
  <script src="lib/jquery/jquery.min.js"></script>
  <script src="lib/bootstrap/js/bootstrap.min.js"></script>
  <!--BACKSTRETCH-->
  <!-- You can use an image of whatever size. This script will stretch to fit in any screen size.-->
  <script type="text/javascript" src="lib/jquery.backstretch.min.js"></script>
  <script>
    $.backstretch("img/login-bg.jpg", {
      speed: 500
    });
  </script>
</body>

